<thead>
  {{view "ember/container" childViews=view.columnHeaderViews tagName="tr"}}
</thead>
{{#if view.hasFooter}}
  <tfoot>
    <tr>
      {{view view.footerContainer colspan=view.columnsCount childViews=view.footerCells}}
    </tr>
  </tfoot>
{{/if}}
<tbody>
  {{#each cells in view.rowCellViews}}
    {{view "ember/container" childViews=cells tagName="tr"}}
  {{/each}}
</tbody>
